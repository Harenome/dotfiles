# apt-get
alias update='apt-get update'
alias upgrade='apt-get upgrade'

# git
alias g='git'
alias gco='g co'
alias gci='g ci'
alias gst='g st'
alias gbr='g br'
alias gdf='g df'
alias gamend='g amend'
alias glg='g lg'
alias glast='g last'

if [[ -f ~/.local/dotfiles/bash/aliases ]]; then
    source ~/.local/dotfiles/bash/aliases
fi

# vim:ft=sh
